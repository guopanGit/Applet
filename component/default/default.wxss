/* component/default/default.wxss */

.default{
  height: 100%;
  padding-top: 320rpx;
  background: #fff;
  text-align: center;
  font-size: 28rpx;
  color: #999;
  box-sizing: border-box;
}

.default-wrap{
  margin: 0 auto;
}

.default-img{
  width: 550rpx;
  height: 340rpx;
}

.btn{
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  padding: 0 30rpx;
  margin-top: 40rpx;
  border-radius: 60rpx;
  font-size: 28rpx;
}

.orange{
  border: 2rpx solid #ff630e;
  color: #ff630e;
}

.gray{
  border: 2rpx solid #999;
  color: #999;
}
