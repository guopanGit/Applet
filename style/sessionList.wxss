/*session list*/
.sessionList{margin-top:-1rpx;}
.ml-24{margin-left:-24rpx;}
.timeTitle{height:77rpx;line-height:77rpx; border-bottom:2rpx solid #e4e4e4;font-size:26rpx;color:#8d8d8d;}
.titleTab{width:30%;text-align: center;}
.tabCur{color:#f35643;position:relative;}
.tabCur::after{content: '';width:180rpx;height:5rpx;background-color:#f35643;border-radius: 2rpx;position:absolute;left:50%;margin-left:-91rpx;bottom:0;}
.timeUl{overflow-x:auto;overflow-scrolling: touch;-webkit-overflow-scrolling: touch;-moz-overflow-scrolling: touch;}
.sesInfoUl{padding-left:48rpx;overflow:hidden;}
.sesInfoLi{/*height:60rpx;*/padding:30rpx 24rpx 30rpx 0;border-bottom:2rpx solid #e4e4e4;position:relative;}
.lastLi{border-bottom:0;}
.sesInfoLi::after{content:'';height:130rpx;width:0;border-left:2rpx dashed #b8b8b8;position:absolute;left:-27rpx;top:30rpx;z-index:1;}
/*.lastLi::affter{content:'';height:60rpx;width:0;border-left:1rpx dashed #b8b8b8;position:absolute;left:-27rpx;top:30rpx;}*/
.topP{font-size:24rpx;}
.nextP{font-size:20rpx;margin-top:16rpx;color:#8d8d8d;}
.smallP{display:block;color:#8d8d8d;padding-top:10rpx;}
.icon_sun{width:23rpx;height:39rpx;background: #f1f1f1 url(data:image/png;base64,) no-repeat;background-size: 23rpx 39rpx;position:absolute;left:-39rpx;top:22rpx;z-index:2;}
.icon_moon{width:16rpx;height:35rpx;background:url(data:image/png;base64,) no-repeat;background-size: 16rpx 35rpx;position:absolute;left:-36rpx;top:30rpx;z-index:2;}
.s1{background-color:#fff;}
.selTicketBtn{display:inline-block;width:86rpx;padding:9rpx 0;/*background-color:#f1f1f1;*/text-align: center;font-size:26rpx;border:2rpx solid;border-radius: 22rpx;}
.grayBtn{border-color:#8d8d8d;color:#8d8d8d;}
.yellowBtn{border-color:#f35643;color:#f35643;}
.yellow_hover{border-color:#ff9584;color:#ff9584;}
.mt6{margin-top:6rpx;}
.mt-3{margin-top:-3rpx;}
.selTicketHints{display:inline-block;width:90rpx;margin-top:10rpx;text-align:center;font-size:18rpx;color:#f35643;}
.nonData{padding:159rpx 0;text-align: center;}
.nonIcon{display: inline-block;width:98rpx;height:82rpx;background: url(data:image/png;base64,) no-repeat;background-size:100% 100%;-webkit-background-size:100% 100%;}
.nonDataHint{margin-top:20rpx;display: block;font-size:26rpx;color:#b8b8b8;}
.fixedHeight{height:77rpx;}