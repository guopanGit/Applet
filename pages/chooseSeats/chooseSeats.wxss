/* pages/chooseSeats/chooseSeats.wxss */
@import '../../style/layout.wxss';

page {
  background-color: #fff;
}

.startTime {
  padding: 0 20rpx;
}

/*抢票倒计时*/

.countDown {
  padding: 0 24rpx;
  height: 66rpx;
}

/*.countIcon{display:inline-block;margin:18rpx 10rpx 0 0;width:24rpx;height:24rpx;}*/

.countText {
  display: inline-block;
  padding: 20rpx 0 0 34rpx;
  color: #8d8d8d;
  font-size: 24rpx;
  background: url(data:image/png;base64,) no-repeat left 18rpx;
  background-size: 24rpx 24rpx;
}

.eventPrice {
  width: 100rpx;
  text-align: right;
}

.curPrice {
  line-height: 38rpx;
  color: #f35643;
  font-size: 30rpx;
}

.filmPrice {
  color: #8d8d8d;
  font-size: 20rpx;
  text-decoration: line-through;
}

.countTime {
  display: inline-block;
  margin-left: 10rpx;
  line-height: 30rpx;
  padding-top: 15rpx;
  font-size: 24rpx;
  color: #f35643;
}

.tb {
  display: inline-block;
  margin: 0 10rpx;
  width: 32rpx;
  padding: 3rpx 0;
  font-size: 24rpx;
  text-align: center;
  background-color: #e0e0e0;
  border-radius: 4rpx;
}

.cinemaName {
  margin-top: 30rpx;
  font-size: 26rpx;
  text-align: center;
}

.filmInfo {
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #8d8d8d;
  text-align: center;
}

.seatsMap {
  margin-top: 40rpx;
  min-height: 800rpx;
  padding-bottom: 80rpx;
  overflow: hidden;
  position: relative;
}

.mapBg {
  width: 2000rpx;
  height: 2000rpx;
  border-radius: 1000rpx;
  background-color: #f1f1f1;
  position: absolute;
  left: 50%;
  top: 21rpx;
  z-index: -1;
  margin-left: -1000rpx;
}

.changeSession {
  margin: 0 auto;
  width: 110rpx;
  height: 42rpx;
  line-height: 42rpx;
  text-align: center;
  color: #f35643;
  font-size: 24rpx;
  background-color: #fff;
  border-radius: 21rpx;
}

.hallName {
  margin: 30rpx 0 40rpx;
  color: #c0c0c0;
  font-size: 22rpx;
  text-align: center;
}

.login-btn {
  text-align: center;
  margin: 0 auto;
}

/*座位图*/
.seatsWrap {
  min-height: 750rpx;
  overflow: scroll;
  position: relative;
  text-align: center;
}

.seatsBox {
  position: absolute;
}

.rowNum {
  position: absolute;
  top: 120rpx;
  left: 24rpx;
  z-index: 9;
  padding: 13rpx 0 30rpx;
  width: 30rpx;
  background-color: rgba(141, 141, 141, 0.6);
  border-radius: 15rpx;
}

.num {
  display: block;
  width: 30rpx;
  height: 54rpx;
  line-height: 35rpx;
  text-align: center;
  font-size: 20rpx;
  color: #fff;
}

/*.mt20{margin-top:20rpx;}*/

.row {
  padding: 0 37rpx 0 27rpx;
}

.middle {
  padding: 0 0 0 27rpx;
  border-right: 1px dashed #c8c8c8;
  position: absolute;
  z-index: -1;
}

.seat {
  display: inline-block;
  width: 58rpx;
  height: 50rpx;
  background: no-repeat center top;
  background-size: 38rpx 30rpx;
}

.fixedBtm {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  z-index: 100;
}

.legend {
  height: 130rpx;
}

.legendWrap {
  height: 120rpx;
  background: #fff;
}

.legendSWrap {
  padding: 30rpx 0;
  text-align: center;
  border-bottom: 1rpx dashed #c8c8c8;
}

.lgdIcon {
  display: inline-block;
  margin-bottom: 4rpx;
  padding-left: 52rpx;
  width: 48rpx;
  height: 30rpx;
  line-height: 30rpx;
  background: no-repeat left top;
  background-size: 38rpx 30rpx;
  font-size: 24rpx;
  color: #8d8d8d;
}

.ml60 {
  margin-left: 60rpx;
}

.btmWrap {
  height: 202rpx;
}

.legendBtm {
  height: 126rpx;
}

/*正常座*/

.empty {
  background-image: url(data:image/png;base64,);
}

/*当前选中*/

.selected {
  background-image: url(data:image/png;base64,);
}

/*已售*/

.soldOut {
  background-image: url(data:image/png;base64,);
}

.buttonWrp {
  padding: 15rpx 24rpx;
  border-top: 1rpx solid #e4e4e4;
}

.selectedWrap {
  padding: 30rpx 24rpx 20rpx;
}

.sum {
  width: 140rpx;
  color: #f35643;
  font-size: 34rpx;
  text-align: right;
}

.selTicket {
  display: inline-block;
  padding: 9rpx 16rpx;
  margin: 0 10rpx 10rpx 0;
  border: 1rpx solid #d4d4d4;
  color: #8d8d8d;
  font-size: 24rpx;
  border-radius: 4rpx;
  position: relative;
}

.selTicket::after, .selTicket::before {
  content: '';
  width: 12rpx;
  height: 12rpx;
  background-color: #fff;
  position: absolute;
  top: 50%;
  margin-top: -7rpx;
  border-radius: 8rpx;
  border: 1rpx solid #d4d4d4;
  border-left-color: #fff;
  border-bottom-color: #fff;
}

.selTicket::before {
  left: -7rpx;
  transform: rotate(45deg);
}

.selTicket::after {
  right: -7rpx;
  transform: rotate(-135deg);
}

/* 模板样式 */
@import "../../template/temp.wxss";
