/*.container {background-color: #F2f2f2;}*/
.userWrap{padding:0 24rpx;height:88rpx;line-height:88rpx;border-top:1rpx solid #e4e4e4;border-bottom:1rpx solid #e4e4e4;font-size:26rpx;background-color:#fff;}
.bannerView{position:relative;}
.address{position:absolute;left:24rpx;top:30rpx;z-index:99;display: block;height:32rpx;line-height:32rpx;padding-left:35rpx;color:#333;font-size:26rpx;background: url(data:image/png;base64,) no-repeat left center; background-size:24rpx 28rpx;}
.users{position:absolute;right:0;top:0;z-index:99;display:block;width:76rpx;height:88rpx;background: url(data:image/png;base64,) no-repeat center center;background-size: 28rpx 28rpx}
.swiper_box, .slide-image{width: 750rpx;height: 248rpx;}
.ad{height:68rpx;line-height:68rpx;padding:0 24rpx;font-size:26rpx;color:#8d8d8d;}
.appLink{float:right;margin-top:14rpx;width:106rpx;height:36rpx;line-height:36rpx;border:2rpx solid #f35643;text-align: center;border-radius:20rpx;font-size:22rpx;color:#f35643;}
.applink_hover{background-color: #f35643;color: #fff;}
.dataLoading{height:606rpx;}
.dataLoading image{display:block;margin:222rpx auto 0;width:160rpx;height:160rpx;}
.dataLoading .p{margin-top:20rpx;text-align:center;font-size:24rpx;color:#8d8d8d;}
.filmList{padding-left:24rpx;}
.films{position:relative;padding:30rpx 24rpx 30rpx 0;border-bottom:2rpx solid #e4e4e4;}
.filmWrap:last-child .films{border-bottom:0;}
.filmImgBox, .filmImg{width:129rpx;height:170rpx;border-radius:8rpx;}
.filmTitle{padding:2rpx 0 22rpx;font-size:30rpx;color:#333;}
.title{padding-top:10rpx;display:inline-block;max-width:325rpx;}
.filmInfo{width:380rpx;padding-top:20rpx;font-size:26rpx;color:#8d8d8d;}
.ntsc{display:inline-block;padding:0 8rpx;height:28rpx;line-height: 28rpx;font-size:20rpx;color:#fff;background-color:#fd8676;border-radius:14rpx;vertical-align: middle;margin-left:16rpx;}
.session{position:absolute;right:24rpx;top:140rpx;width:120rpx;height:48rpx;line-height:48rpx;background:-webkit-linear-gradient(left, #fd8e47, #f35643);border-radius:24rpx;color:#fff;font-size:26rpx;text-align:center;}
.session_hover{background:-webkit-linear-gradient(left, #fb7e38, #e0381c);}
.slideBtn{display: inline-block;width:10rpx;height:10rpx;border-left:2rpx solid #fff;border-bottom:2rpx solid #fff;transform: rotate(-45deg);vertical-align: middle;margin-left:6rpx;margin-bottom:6rpx;}
.slideBtnCur{transform: rotate(135deg);margin-bottom:0;}
/*session list*/
@import "../../style/sessionList.wxss";
.stop{height:70rpx;position:relative;}
.stop::after{content: '';width:16rpx;height:16rpx;border-top:3rpx solid #c8c8c8;border-right:3rpx solid #c8c8c8;transform: rotate(-45deg);position:absolute;left:50%;margin-left:-5rpx;top:30rpx;}
.booking{width:130rpx;}
/* template style */
@import "../../template/temp.wxss";
.ciri{padding:60rpx 24rpx 25rpx 0;}
.ciri_tab{width:74rpx;padding:3rpx 0 4rpx;font-size:14rpx;color:#fff;background:#76d2fd;text-align:center;border-radius:11rpx;position:absolute;left:0;top:25rpx;}
.none{display:none;}
/* 去掉默认滚动条 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

