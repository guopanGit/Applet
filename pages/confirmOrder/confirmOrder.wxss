/* pages/confirmOrder/confirmOrder.wxss */
@import '../confirmOrder/chooseCard.wxss';

page {
  background: #fff;
}

.remainedTime {
  height: 58rpx;
  line-height: 58rpx;
  color: #f35643;
  text-align: center;
  font-size: 24rpx;
}

.timeIcon {
  display: inline-block;
  margin-right: 10rpx;
  width: 20rpx;
  height: 20rpx;
  border: 2rpx solid #f35643;
  border-radius: 12rpx;
  position: relative;
}

.timeIcon::after {
  content: '';
  position: absolute;
  left: 5rpx;
  top: 5rpx;
  width: 6rpx;
  height: 6rpx;
  border-right: 2rpx solid #f35643;
  border-bottom: 2rpx solid #f35643;
}

.filmInfo {
  padding: 30rpx 24rpx;
  border-bottom: 1rpx solid #e4e4e4;
}

