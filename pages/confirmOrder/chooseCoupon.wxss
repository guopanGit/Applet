@import 'chooseCard.wxss';
@import '../store/index.wxss';
.coupon{margin: 30rpx 0;}
.couponInfo{height:206rpx;background-color:#fff;position:relative;border-radius: 8rpx;}
.couponType{padding-left:38rpx;width:130rpx;height:206rpx;position:relative;color:#f1f1f1;border-radius: 8rpx;}
.couponType::before, .couponType::after{content:'';display:block;width:16rpx;height:16rpx;background-color:#f1f1f1;border-radius:10rpx;position:absolute;z-index: 2;}
.couponType::before{right:-8rpx;top:-8rpx;}
.couponType::after{right:-8rpx;bottom:-8rpx;}
.yelTBg{background-color:#f6ca43}
.blueTBg{background-color:#46b6f6}
.radTBg{background-color:#fb604e}
.grayTBg{background-color:#cfcfcf;}
.typeTit{padding-top:16rpx;font-size:20rpx;}
.sum{padding:50rpx 0;font-size:40rpx;color:#fff;}
.mCoupon{font-size:18rpx;padding-left:22rpx;background:url(data:image/png;base64,) no-repeat left center;background-size:18rpx 18rpx;}
.sCoupon{font-size:18rpx;padding-left:22rpx;background:url(data:image/png;base64,) no-repeat left center; background-size:18rpx 18rpx}
.infoWrapper{padding-left:38rpx;padding-right:20rpx;height:206rpx;position:relative;}
.infoWrapper::after{content:'';position:absolute;right:-14rpx;top:50%;margin-top:-14rpx;display:block;width:28rpx;height:28rpx;background-color:#f1f1f1;border-radius: 20rpx;}
.couponName{padding:84rpx 0 30rpx;font-size:30rpx;color:#333;}
.couponDeadline{font-size:20rpx;color:#8d8d8d;line-height: 64rpx;}
.couponDeadline1{line-height: 1.4}
.couJiao{display:inline-block;width:10rpx;height:10rpx;border-top:1px solid #8d8d8d;border-right:1px solid #8d8d8d;transform: rotate(45deg);position:relative;}
.bb{border-bottom:1px dashed #e6e6e6;}
.mulSelectItem{height:46rpx;line-height:46rpx;padding-left:38rpx;font-size:18rpx;background:#fff;border-radius: 8rpx;color:#8d8d8d;}
.selIcon{display:inline-block;margin-bottom:4rpx;margin-right:6rpx;width:16rpx;height:16rpx;border:1px solid #8d8d8d;border-radius: 10rpx;position:relative;vertical-align: middle;}
.selIcon::after{content: "";width:6rpx;height:4rpx;border-left:1px solid #8d8d8d;border-bottom:1px solid #8d8d8d;transform: rotate(-50deg);position:absolute;left:5rpx;top:5rpx;}
 page{background:#f1f1f1;}
