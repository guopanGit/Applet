page{background: #fff;position: relative}
.header{width:100%;height:250rpx;line-height:250rpx;text-align:center;}
.cinemaLogo{height: 130rpx;margin-top:60rpx;}
.title{padding:0 24rpx;height:76rpx;line-height: 76rpx;font-size:28rpx;background: #f1f1f1;position: relative;}
.selectedCity{padding-left:24rpx;color:#8d8d8d;}
.reloaded{display:-webkit-inline-box; width: 28rpx;height: 30rpx;background: url(data:image/png;base64,)no-repeat;background-size:28rpx 30rpx;vertical-align: middle;position: absolute;right: 24rpx;top: 16rpx;}
/*.cityBox{padding-left:24rpx; }*/
.currentCity{display:flex;height: 96rpx;padding:0 24rpx;font-size:28rpx;border-bottom: 1px solid #e4e4e4;}
.txt{line-height: 96rpx;}
.selectCity{display:block;padding: 10rpx 42rpx 10rpx 14rpx;border: 1px solid #f35643;color: #f35643;font-size: 24rpx;margin-left: 26rpx;border-radius: 36rpx;position: relative;height: 24rpx;margin-top: 25rpx}
.selectCity::after{content: "";display: block;width: 14rpx;height: 14rpx;position: absolute;right: 16rpx;top: 9rpx;border-bottom: 1px solid #f35643;border-left:1px solid #f35643;-webkit-transform: rotate(-45deg);}
.cinemaList{height: 74rpx;padding: 26rpx 24rpx;border-bottom: 1px solid #e4e4e4;}
.cinemaName{font-size: 26rpx;}
.adress{display: flex;color: #8d8d8d;font-size: 24rpx;margin-top: 19rpx;}
.adressMsg{flex: 1;height: 25rpx;line-height: 26rpx;}
.cityList{position: fixed;bottom: 0;left: 0;right: 0;background: #fff;z-index: 11;}
.closeBtn{width: 110rpx;height: 110rpx;background: #262626;position: absolute;left: 50%;margin-left: -55rpx;border-radius: 55rpx;text-align: center;margin-top: -55rpx;z-index: 12;}
.imgClose{width: 90rpx;height: 90rpx;margin: 10rpx 0;opacity: 1}
.curCity{margin-top: 84rpx;height: 60rpx;line-height: 60rpx;background: #f3f3f3;font-size: 28rpx;color: #b5b5b5;padding: 0 24rpx;}
.city{display:block;height: 87rpx;line-height: 87rpx;margin: 0 24rpx;font-size:28rpx;}
/*.citys{padding:0 36rpx 0 24rpx;}*/
.cityN{border-top:1px solid #e4e4e4;}
.mtop0{margin-top: 0;}
.wordBox{position: absolute;right: 24rpx;top: 27rpx;font-size: 24rpx;color: #333;}
.word{margin-bottom: 24rpx;}
.none{display: none;}
@import "../../style/layout.wxss";
.whirl{animation: rotation 1s linear infinite;-webkit-animation: rotation 1s linear infinite;}
@-webkit-keyframes rotation{
    from {-webkit-transform: rotate(0deg);}
    to {-webkit-transform: rotate(360deg);}
}
.cityView{max-height:200px;}
.scrollHead{height:231rpx;}
/* template style */
@import "../../template/temp.wxss";
