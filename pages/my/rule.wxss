@import 'my.wxss';

.rule-wrap{
  padding: 0 30rpx;
}

.rule-text{
  font-size: 30rpx;
  line-height: 60rpx;
  color: #494949;
  padding-left: 30rpx;
  position: relative;
}

.rule-text::before{
  content: "";
  display: block;
  width:8rpx;
  height: 8rpx;
  background: #000;
  border-radius: 20px;
  position: absolute;
  left: 0;
  top:26rpx;
}

.rule-icon{
  display: inline-block;
  width: 180rpx;
  height: 180rpx;
  margin: 0 auto;
  margin-top: 110rpx;
  margin-bottom: 80rpx;
  background: url(data:image/png;base64,)no-repeat center center;background-size:180rpx 180rpx;}